{
  "problemMatcher": [
    {
      "owner": "tsc-paren",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^(.*)\\((\\d+),(\\d+)\\): error TS(\\d+): (.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "code": 4,
          "message": 5
        }
      ]
    },
    {
      "owner": "tsc-colon",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^(.*):(\\d+):(\\d+) - error TS(\\d+): (.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "code": 4,
          "message": 5
        }
      ]
    },
    {
      "owner": "tsc-warning",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^(.*):(\\d+):(\\d+) - warning TS(\\d+): (.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "code": 4,
          "message": 5
        }
      ]
    },
    {
      "owner": "tsc-info",
      "severity": "info",
      "pattern": [
        {
          "regexp": "^(.*):(\\d+):(\\d+) - info TS(\\d+): (.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "code": 4,
          "message": 5
        }
      ]
    },
    {
      "owner": "tsc-generic",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^(.+\\.tsx?):(\\d+):(\\d+) - (.+)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4
        }
      ]
    },
    {
      "owner": "tsc-summary",
      "pattern": [
        {
          "regexp": "^Found (\\d+) errors?\\.",
          "message": 1
        }
      ]
    }
  ]
}
